<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apps.view.pages.personal.notifications.NotificationPage">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@../../../../../Images/images.jfif" />
         </image>
      </ImageView>
      <Text fill="#0c6f08" layoutX="162.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose each notification you want to see" wrappingWidth="476.9375">
         <font>
            <Font name="Lucida Calligraphy Italic" size="22.0" />
         </font>
      </Text>
      <Button fx:id="newRequests" layoutX="294.0" layoutY="108.0" mnemonicParsing="false" onAction="#newRequests" prefHeight="58.0" prefWidth="212.0" text="New Requests" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="myRequests" layoutX="294.0" layoutY="217.0" mnemonicParsing="false" onAction="#myRequests" prefHeight="58.0" prefWidth="212.0" text="My Requests" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="580.0" layoutY="480.0" mnemonicParsing="false" onAction="#back" prefHeight="58.0" prefWidth="149.0" text="Back" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="60.0" layoutY="480.0" mnemonicParsing="false" onAction="#exit" prefHeight="58.0" prefWidth="149.0" text="Exit" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="mainMenu" layoutX="325.0" layoutY="480.0" mnemonicParsing="false" onAction="#mainMenu" prefHeight="58.0" prefWidth="149.0" text="MainMenu" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="followingState" layoutX="294.0" layoutY="331.0" mnemonicParsing="false" onAction="#followingState" prefHeight="58.0" prefWidth="212.0" text="Following State" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
