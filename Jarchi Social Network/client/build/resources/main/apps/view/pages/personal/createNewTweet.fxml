         <font>
            <Font name="Britannic Bold" size="18.0" />
         </font>
      </Button>
      <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" layoutX="300.0" layoutY="77.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="upload" layoutX="359.0" layoutY="137.0" mnemonicParsing="false" onAction="#upload" prefHeight="30.0" prefWidth="83.0" text="upload" textAlignment="CENTER">
