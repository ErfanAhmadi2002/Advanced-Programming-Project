<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apps.view.pages.personal.CreateNewTweet">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0">
         <image>
            <Image url="@../../../../Images/images.jfif" />
         </image>
      </ImageView>
      <Text fill="#0c6f08" layoutX="204.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You can create your new Tweet  " wrappingWidth="393.9375">
         <font>
            <Font name="Lucida Calligraphy Italic" size="22.0" />
         </font>
      </Text>
      <Button fx:id="back" layoutX="580.0" layoutY="480.0" mnemonicParsing="false" onAction="#back" prefHeight="58.0" prefWidth="149.0" text="Back" textAlignment="CENTER">
         <font>
            <Font name="Britannic Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="65.0" layoutY="480.0" mnemonicParsing="false" onAction="#exit" prefHeight="58.0" prefWidth="149.0" text="Exit" textAlignment="CENTER">
